{
  "routes": [
    {
      "route": "/api/webhook/ask-foreman/*",
      "rewrite": "https://workflows.saxtechnology.com/webhook/ask-foreman/",
      "methods": ["GET", "POST", "OPTIONS"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/config",
      "methods": ["GET"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/admin.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,js,json,png,jpg,jpeg,gif,ico,svg}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
