    {
      "parameters": {
        "jsCode": "const count = $('code-clear-prepare').item.json.count || 0;\n\nreturn {\n  success: true,\n  deletedCount: count,\n  message: `Successfully cleared index. ${count} document(s) removed.`\n};"
      },
