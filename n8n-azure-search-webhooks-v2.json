        "jsonBody": "{\n  \"search\": \"*\",\n  \"top\": 1000,\n  \"select\": \"id\"\n}",
        "options": {}
      },
      "id": "http-get-all",
      "name": "Get All Documents",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [450, 800]
    },
    {
      "parameters": {
        "jsCode": "const documents = $input.first().json.value || [];\n\nif (documents.length === 0) {\n  return {\n    success: true,\n    deletedCount: 0,\n    message: \"Index is already empty\",\n    skip: true\n  };\n}\n\nconst batch = {\n  value: documents.map(doc => ({\n    '@search.action': 'delete',\n    id: doc.id\n  }))\n};\n\nreturn {\n  batch: batch,\n  count: documents.length,\n  skip: false\n};"
      },
      "id": "code-prepare-clear",
      "name": "Prepare Clear Batch",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [650, 800]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
