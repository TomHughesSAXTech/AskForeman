    {
      "parameters": {
        "jsCode": "const documentIds = $input.first().json.documentIds || [];\n\nif (!Array.isArray(documentIds) || documentIds.length === 0) {\n  return {\n    success: false,\n    message: \"No document IDs provided\"\n  };\n}\n\nconst batch = {\n  value: documentIds.map(id => ({\n    '@search.action': 'delete',\n    id: id\n  }))\n};\n\nreturn batch;"
      },
